<?xml version="1.0"?>
<robot name="barista_robot">

  <material name="blue">
      <color rgba="0.0 0.0 1.0 1"/>
  </material>

  <material name="black">
      <color rgba="0.0 0.0 0.0 1"/>
  </material>

  <material name="cany">
      <color rgba="0.088656 0.428691 0.491021 1"/>
  </material>

  <material name="red">
      <color rgba="1.0 0.0 0.0 1"/>
  </material>

  <material name="green_light">
      <color rgba="0.0 1.0 0.0 1"/>
  </material>

  <material name="green_dark">
    <color rgba="0.0 0.5 0.0 1"/>
  </material>

  <material name="white">
      <color rgba="1.0 1.0 1.0 1"/>
  </material>

  <link name="base_link">
      <inertial>
        <origin xyz="0 0 0.05" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

    <!-- chassis -->
    <link name="chassis">
        <visual>
            <geometry>
                <cylinder length="0.155" radius="0.178"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="5.0"/>
            <!-- <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.15"/> -->
            <inertia ixx="0.04961541666666667" ixy="0" ixz="0" iyy="0.04961541666666667" iyz="0" izz="0.07920999999999999"/>
        </inertial>
    </link>

    <joint name="base_link_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0775" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="chassis"/>
        <axis xyz="0.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <!-- FRONT indicator -->
    <link name="front_indicator">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.02 0.01"/>
            </geometry>
            <material name="cany"/>
        </visual>
    </link>

    <joint name="joint_front_indicator" type="fixed">
        <origin xyz="0.18 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="chassis"/>
        <child link="front_indicator"/>
    </joint>

    <!-- Laser Scanner -->
    <link name="laser_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <!-- <cylinder length="0.04" radius="0.025"/> -->
                <mesh filename="package://barista_robot_description/meshes/sick_tim551.dae" scale="0.6 0.6 0.6"/>
            </geometry>
            <!-- <material name="black"/> -->
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <!-- <box size="0.5 0.5 0.5"/> -->
                <!-- <cylinder length="0.04" radius="0.025"/> -->
                <mesh filename="package://barista_robot_description/meshes/sick_tim551.dae" scale="0.6 0.6 0.6"/>
            
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.0005"/>
        </inertial>
    </link>

    <joint name="joint_laser" type="fixed">
        <origin xyz="0.15 0.0 0.077" rpy="0.0 0.0 0.0"/>
        <parent link="chassis"/>
        <child link="laser_link"/>
    </joint>

    <!-- Drive Wheels -->
    <link name="left_wheel">      
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.0206" radius="0.0352"/>
            </geometry>
            <material name="red"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.00017256166666666667" ixy="0" ixz="0" iyy="0.00017256166666666667" iyz="0" izz="0.00030976"/>
        </inertial>
    </link>

    <joint name="joint_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.14 -0.070"/>
        <child link="left_wheel"/>
        <parent link="chassis"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <!-- <joint_properties damping="1.0" friction="1.0"/> -->
        <joint_properties damping="0.5" friction="0.5"/>
    </joint>

    <link name="right_wheel">      
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.0206" radius="0.0352"/>
            </geometry>
            <material name="red"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <!-- <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/> -->
            <inertia ixx="0.00017256166666666667" ixy="0" ixz="0" iyy="0.00017256166666666667" iyz="0" izz="0.00030976"/>
        </inertial>
    </link>

    <joint name="joint_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.14 -0.070"/>
        <child link="right_wheel"/>
        <parent link="chassis"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <!-- <joint_properties damping="1.0" friction="1.0"/> -->
        <joint_properties damping="0.5" friction="0.5"/>
    </joint>

    <!-- Front Caster Wheel-->
    <link name="front_yaw_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.005" radius="0.01"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="4.000000000000001e-06" ixy="0" ixz="0" iyy="4.000000000000001e-06" iyz="0" izz="4.000000000000001e-06"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.005" radius="0.01"/>
            </geometry>
        </collision>
    </link>

    <joint name="front_yaw_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0.12 0.0 -0.070" />
        <parent link="chassis" />
        <child link="front_yaw_link" />
        <axis xyz="0 0 1" />
        <limit effort="1000.0" velocity="100.0" />
        <!-- <dynamics damping="0.0" friction="0.1"/> -->
        <dynamics damping="2.0" friction="0.5"/>
    </joint>

    <link name="front_roll_link">
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
            <material name="green_dark"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="4.000000000000001e-06" ixy="0" ixz="0" iyy="4.000000000000001e-06" iyz="0" izz="4.000000000000001e-06"/>
        </inertial>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </collision>
    </link>

    <joint name="front_roll_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="front_yaw_link" />
        <child link="front_roll_link" />
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="100.0" />
        <!-- <dynamics damping="0.0" friction="0.1"/> -->
        <dynamics damping="2.0" friction="0.5"/>
    </joint>

    <!-- Back Caster Wheel -->
    <link name="back_yaw_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.005" radius="0.01"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="4.000000000000001e-06" ixy="0" ixz="0" iyy="4.000000000000001e-06" iyz="0" izz="4.000000000000001e-06"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.005" radius="0.01"/>
            </geometry>
        </collision>
    </link>

    <joint name="back_yaw_joint" type="continuous">
        <origin rpy="0 0 0" xyz="-0.12 0.0 -0.070" />
        <parent link="chassis" />
        <child link="back_yaw_link" />
        <axis xyz="0 0 1" />
        <limit effort="1000.0" velocity="100.0" />
        <!-- <dynamics damping="0.0" friction="0.1"/> -->
        <dynamics damping="2.0" friction="0.5"/>
    </joint>

    <link name="back_roll_link">
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
            <material name="green_dark"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <!-- <mass value="0.2"/> -->
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="4.000000000000001e-06" ixy="0" ixz="0" iyy="4.000000000000001e-06" iyz="0" izz="4.000000000000001e-06"/>
        </inertial>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.0352"/>
            </geometry>
        </collision>
    </link>

    <joint name="back_roll_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="back_yaw_link" />
        <child link="back_roll_link" />
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="100.0" />
        <!-- <dynamics damping="0.0" friction="0.1"/> -->
        <dynamics damping="2.0" friction="0.5"/>
    </joint>

    <!-- Standoff rods -->
    <link name="standoff_front_left">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.010" length="0.220"/>
            </geometry>
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
        </inertial>
    </link>

    <joint name="joint_standoff_front_left" type="fixed">
        <origin rpy="0 0 0" xyz="0.065 0.12 0.1875"/>
        <child link="standoff_front_left"/>
        <parent link="chassis"/>
    </joint>

    <link name="standoff_front_right">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.010" length="0.220"/>
            </geometry>
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
        </inertial>
    </link>

    <joint name="joint_standoff_front_right" type="fixed">
        <origin rpy="0 0 0" xyz="0.065 -0.12 0.1875"/>
        <child link="standoff_front_right"/>
        <parent link="chassis"/>
    </joint>

    <link name="standoff_back_left">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.010" length="0.220"/>
            </geometry>
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
        </inertial>
    </link>

    <joint name="joint_standoff_back_left" type="fixed">
        <origin rpy="0 0 0" xyz="-0.065 0.12 0.1875"/>
        <child link="standoff_back_left"/>
        <parent link="chassis"/>
    </joint>

    <link name="standoff_back_right">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.010" length="0.220"/>
            </geometry>
            <material name="black"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <!-- <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/> -->
            <inertia ixx="0.00040583333333333335" ixy="0" ixz="0" iyy="0.00040583333333333335" iyz="0" izz="5e-06"/>
        </inertial>
    </link>

    <joint name="joint_standoff_back_right" type="fixed">
        <origin rpy="0 0 0" xyz="-0.065 -0.12 0.1875"/>
        <child link="standoff_back_right"/>
        <parent link="chassis"/>
    </joint>

    <!-- Cup holder tray -->
    <link name="cup_holder_tray">
        <visual>
            <geometry>
                <cylinder length="0.090" radius="0.160"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <!-- <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.03"/> -->
            <inertia ixx="0.007075" ixy="0" ixz="0" iyy="0.007075" iyz="0" izz="0.0128"/>
        </inertial>
    </link>

    <joint name="joint_cup_holder_tray" type="fixed">
        <origin xyz="0.0 0.0 0.3425" rpy="0.0 0.0 0.0"/>
        <parent link="chassis"/>
        <child link="cup_holder_tray"/>
    </joint>

</robot>