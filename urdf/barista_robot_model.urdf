        </geometry>
        <material name="green_dark"/>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="4.95616e-05" ixy="0" ixz="0" iyy="4.95616e-05" iyz="0" izz="4.95616e-05"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
